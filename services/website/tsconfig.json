{
  "include": ["./src/**/*"],
  "exclude": ["node_modules/*"],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "typeRoots": ["typings"],
    "types": ["svelte", "@sapper"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
