{
  "name": "website",
  "description": "Website",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "NODE_ENV=production sapper export --legacy",
    "postexport": "node generate-sitemap.js",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "images": "run-p images:*",
    "images:jpgtowebp": "find ./static/blog-posts/ -type f -and -iname \"*.jpg\" -exec bash -c 'convert \"$0\" -quiet -quality 90 \"$(dirname \"$0\")/$(basename \"$0\" .jpg).webp\"' {} \\;",
    "images:jpgtojp2": "find ./static/blog-posts/ -type f -and -iname \"*.jpg\" -exec bash -c 'convert \"$0\" -quiet -quality 0 \"$(dirname \"$0\")/$(basename \"$0\" .jpg).jp2\"' {} \\;"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "cypress": "^4.8.0",
    "front-matter": "^4.0.2",
    "mdsvex": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.9.7",
    "tailwindcss": "^1.4.6"
  }
}
